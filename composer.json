{
    "name": "jmolivas/drupal-console-global-extend",
    "description": "Drupal Console library extend example",
    "type": "drupal-console-library",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Jesus Manuel Olivas",
            "email": "jesus.olivas@gmail.com"
        }
    ],
    "require": {
        "mustangostang/spyc": "^0.6.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"Drupal\\Console\\GlobalExtend\\": "src"}
    },
    "require-dev": {
        "drupal/console-core": "~1.0",
        "composer/installers": "^1.2"
    },
    "scripts": {
        "post-install-cmd": [
            "Drupal\\Console\\GlobalExtend\\Plugin\\ScriptHandler::dump"
        ],
        "post-update-cmd": [
            "Drupal\\Console\\GlobalExtend\\Plugin\\ScriptHandler::dump"
        ]
    }
}
